{
  "name": "webstorage-utils",
  "version": "1.2.8",
  "description": "web storage utils",
  "main": "dist/web-storage-util.umd.js",
  "module": "dist/web-storage-util.es5.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --ext .ts src",
    "lint:fix": "yarn lint --fix",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "prepublishOnly": "yarn build",
    "generateDoc": "typedoc --options typedoc.js",
    "bump-version": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --ext .ts --fix"
    ]
  },
  "keywords": [
    "localstorage",
    "sessionstorage",
    "webstorage",
    "storage"
  ],
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.13.0"
  },
  "author": "Zn",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chenzn1/webstorage-utils.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "lint-staged": "^13.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.8.4"
  }
}
